@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ManyQuery</title>
</head>
<body>
    <div>
           <table>
            <tr>
                <td>姓名</td>
                <td>地址</td>
                <td>邮箱</td>
            </tr>
            @foreach (var item in (List<Domain.Users>)ViewData["userList"])
            {
                <tr>
                    <td>@item.UserName</td>
                    <td>@item.Address</td>
                    <td>@item.Email</td>
                </tr>    
            }
        </table>
    </div>
      <div>
           <table>
            <tr>
                <td>名称</td>
                <td>描述</td>
                <td>创建时间</td>
            </tr>
            @foreach (var item in (List<Domain.Product>)ViewData["productList"])
            {
                <tr>
                    <td>@item.ProductName</td>
                    <td>@item.ProductDesc</td>
                    <td>@item.CreateTime</td>
                </tr>    
            }
        </table>
    </div>
</body>
</html>
